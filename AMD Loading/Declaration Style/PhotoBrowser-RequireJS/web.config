<?xml version="1.0"?>
<configuration>
  <system.webServer>
    <staticContent>
      <!-- Needed to enable IIS to serve .json files -->
      <mimeMap fileExtension=".json" mimeType="application/json"/>
    </staticContent>
  </system.webServer>
    <system.web>
      <!-- Minimum needed for debugging support -->
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>

</configuration>
